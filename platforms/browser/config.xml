<?xml version='1.0' encoding='utf-8'?>
<widget id="com.BussApp.mx" version="0.7.5.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>BussApp</name>
    <description>
        App Transporte publico Guadalajara
    </description>
    <author email="bernagaufo@hotmail.com" href="bernagaufo.com">
        Rafael Robles B.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*://*.googleapis.com/*" subdomains="true" />
    <access origin="*://*.google.com/*" subdomains="true" />
    <access origin="*://*.googleusercontent.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="http://google.com" />
    <access origin="https://google.com" />
    <access origin="https://*.google.com" />
    <access launch-external="yes" origin="tel:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" launch-external="yes" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyAFhnq5lXu7GPt-SDROFbJp8k6tqgWFKIg" />
